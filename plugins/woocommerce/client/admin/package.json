	"config": {
		"ci": {
			"lint": {
				"command": "lint",
				"changes": "client/**/*.{js,ts,tsx,scss}"
			},
			"tests": [
				{
					"name": "JavaScript",
					"command": "test:js",
					"changes": [
						"jest.config.js",
						"webpack.config.js",
						"babel.config.js",
						"tsconfig.json",
						"client/**/*.{js,jsx,ts,tsx,scss,json}"
					],
					"events": [
						"pull_request",
						"push"
					]
				}
			]
		}
	},
	"wireit": {
		"build:project:bundle": {
			"command": "webpack",
			"clean": "if-file-deleted",
			"env": {
				"NODE_ENV": {
					"external": true,
					"default": "production"
				},
				"WC_ADMIN_PHASE": {
					"external": true,
					"default": "core"
				}
			},
			"files": [
				"webpack.config.js",
				"tsconfig.json",
				"client/**/*.{js,jsx,ts,tsx,scss}"
			],
			"output": [
				"build"
			],
			"dependencies": [
				"dependencyOutputs"
			]
		},
		"watch:build:project:bundle": {
			"command": "webpack --watch",
			"service": true
		},
		"dependencyOutputs": {
			"allowUsuallyExcludedPaths": true,
			"files": [
				"package.json",
